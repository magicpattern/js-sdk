{
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@magicpattern/js-sdk",
  "description": "Integrate MagicPattern into your web app",
  "author": "Jim Raptis",
  "module": "dist/integration.esm.js",
  "homepage": "https://github.com/magicpattern/js-sdk",
  "keywords": [
    "magicpattern",
    "design tool",
    "pattern",
    "gradient",
    "blob"
  ],
  "bugs": "https://github.com/magicpattern/js-sdk/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/magicpattern/js-sdk.git"
  },
  "size-limit": [
    {
      "path": "dist/integration.cjs.production.min.js",
      "limit": "2 KB"
    },
    {
      "path": "dist/integration.esm.js",
      "limit": "2 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "husky": "^6.0.0",
    "rollup-plugin-less": "^1.1.3",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}